{
  "name": "ngx-tiny",
  "version": "10.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lint && npm run package && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:after-build": "node scripts/docs-prep",
    "docs:build": "ng build --prod --output-path docs --base-href https://aavanzyl.github.io/ngx-tiny/",
    "docs": "npm run docs:build && npm run docs:after-build",
    "clear:dist" : "node scripts/clear-dist",
    "package": "npm run clear:dist && run-s package:*",
    "package:code-highlight": "ng build @ngx-tiny/code-highlight",
    "package:single-select": "ng build @ngx-tiny/single-select && scss-bundle -c projects/ngx-single-select -p projects/ngx-single-select",
    "package:multi-select": "ng build @ngx-tiny/multi-select && scss-bundle -c projects/ngx-multi-select -p projects/ngx-multi-select",
    "package:switch-input": "ng build @ngx-tiny/switch-input && scss-bundle -c projects/ngx-switch-input -p projects/ngx-switch-input",
    "package:nav-drawer": "ng build @ngx-tiny/nav-drawer && scss-bundle -c projects/ngx-nav-drawer -p projects/ngx-nav-drawer",
    "package:quill-editor": "ng build @ngx-tiny/quill-editor",
    "package:clipboard": "ng build @ngx-tiny/clipboard",
    "package:date-picker": "ng build @ngx-tiny/date-picker && scss-bundle -c projects/ngx-date-picker -p projects/ngx-date-picker",
    "package:time-picker": "ng build @ngx-tiny/time-picker && scss-bundle -c projects/ngx-time-picker -p projects/ngx-time-picker",
    "package:tabs": "ng build @ngx-tiny/tabs && scss-bundle -p projects/ngx-tabs -c projects/ngx-tabs"
  },
  "repository": "https://github.com/aavanzyl/ngx-tiny",
  "author": "Anton Van Zyl <aavanzyl@gmail.com>",
  "bugs": {
    "url": "https://github.com/aavanzyl/ngx-tiny/issues"
  },
  "homepage": "https://aavanzyl.github.io/ngx-tiny",
  "keywords": [
    "angular",
    "ng",
    "ng2",
    "ngx",
    "angular2",
    "angular8",
    "ngx-tiny"
  ],
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.12",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~10.0.12",
    "@angular/compiler": "~10.0.12",
    "@angular/core": "~10.0.12",
    "@angular/forms": "~10.0.12",
    "@angular/localize": "^10.0.12",
    "@angular/platform-browser": "~10.0.12",
    "@angular/platform-browser-dynamic": "~10.0.12",
    "@angular/router": "~10.0.12",
    "date-fns": "^2.15.0",
    "fs-extra": "^8.1.0",
    "ngx-gtag": "^7.0.2",
    "prismjs": "^1.21.0",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular-devkit/build-ng-packagr": "~0.1000.7",
    "@angular/cli": "^10.0.7",
    "@angular/compiler-cli": "~10.0.12",
    "@angular/language-service": "~10.0.12",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "npm-run-all": "^4.1.2",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.1.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
